{
  "expo": {
    "name": "Blank Template",
    "slug": "Find_POIS",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist":{
        "NSLocationWhenInUseUsageDescription": "need location access to find things nearby",
        "NSLocationAlwaysUsageDescription": "Allow Expo experiences to use your location"}
    },
      "android": {
        "icon": "./assets/pois.png",
        "package": "local.company.applet"
      }
  
  }
}
